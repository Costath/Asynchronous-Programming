<Window x:Class="Question_1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Question_1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="592" Loaded="Window_Loaded" WindowStartupLocation="CenterScreen">
    <Grid>
        <DataGrid x:Name="StockDataGrid" AutoGenerateColumns="True" Margin="0,45,0,21" />
        <ComboBox x:Name="SymbolComboBox" HorizontalAlignment="Left" Margin="156,12,0,0" VerticalAlignment="Top" Width="120" SelectionChanged="SymbolComboBox_SelectionChanged"/>
        <Label Content="Search:" HorizontalAlignment="Left" Margin="96,10,0,0" VerticalAlignment="Top"/>
        <ProgressBar x:Name="LoadProgressBar" HorizontalAlignment="Center" Height="21" Margin="0,413,0,0" VerticalAlignment="Top" Width="592"/>

        <!--</DataGrid>-->

    </Grid>
</Window>
